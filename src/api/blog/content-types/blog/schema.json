{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "conclusion": {
      "type": "richtext",
      "required": true
    },
    "time_to_read": {
      "type": "integer",
      "required": true
    },
    "introduction": {
      "type": "text",
      "required": true,
      "minLength": 25
    },
    "footer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::footer.footer"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "slug": {
      "type": "uid",
      "required": true
    }
  }
}
